}
var warned = {};
function printDeprecationWarning(functionName) {
  if (warned[functionName]) {
    return;
  }
  console.warn(`
  Deprecation Warning:
  Use of ${functionName} is not recommended and will be deleted in future versions of @testing-library/react-native.
  `);
