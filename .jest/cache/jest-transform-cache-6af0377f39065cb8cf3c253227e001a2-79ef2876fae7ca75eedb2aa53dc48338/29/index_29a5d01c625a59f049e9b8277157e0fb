'use strict';

var minIndent = require('min-indent');
module.exports = function (string) {
  var indent = minIndent(string);
  if (indent === 0) {
    return string;
  }
  var regex = new RegExp(`^[ \\t]{${indent}}`, 'gm');
  return string.replace(regex, '');
};
